Cell[TextData[{
 "This week you will learn the all important \[OpenCurlyDoubleQuote]chain \
rule\[CloseCurlyDoubleQuote]. This theorem states that if ",
 Cell[BoxData[
  FormBox["f", TraditionalForm]]],
 " is differentiable at ",
 Cell[BoxData[
  FormBox[
   RowBox[{"g", "(", "a", ")"}], TraditionalForm]]],
 " and ",
 Cell[BoxData[
  FormBox["g", TraditionalForm]]],
 " is differentiable at ",
 Cell[BoxData[
  FormBox["a", TraditionalForm]]],
 " then ",
 Cell[BoxData[
  FormBox[
   RowBox[{"f", "\[SmallCircle]", "g"}], TraditionalForm]]],
 " is differentiable at ",
 Cell[BoxData[
  FormBox["a", TraditionalForm]]],
 " and "
}], "Text",
 CellChangeTimes->{{3.525952898594*^9, 3.525952999686*^9}, {3.52595312373*^9, 
   3.525953138769*^9}, {3.6639138286249514`*^9, 3.6639138739462748`*^9}, 
   3.663914132930091*^9, {3.6639159493086653`*^9, 3.6639159501387916`*^9}, {
   3.663916244670891*^9, 3.6639162553068423`*^9}}],

Cell[TextData[Cell[BoxData[
 FormBox[GridBox[{
    {
     RowBox[{
      RowBox[{
       SuperscriptBox[GridBox[{
          {
           RowBox[{"(", 
            RowBox[{"f", "\[SmallCircle]", "g"}], ")"}]}
         },
         GridBoxAlignment->{"Columns" -> {{"Center"}}}], "\[Prime]"], 
       RowBox[{"(", "a", ")"}]}], "=", 
      RowBox[{
       RowBox[{
        SuperscriptBox["f", "\[Prime]"], "(", 
        RowBox[{"g", "(", "a", ")"}], ")"}], 
       RowBox[{
        SuperscriptBox["g", "\[Prime]"], "(", "a", ")"}]}]}]}
   },
   GridBoxItemSize->{"Columns" -> {{
        Scaled[0.96]}}}], TraditionalForm]]]], "Text",
 CellChangeTimes->{{3.525953040336*^9, 3.525953109102*^9}, {3.525953145961*^9,
   3.5259531632469997`*^9}}],

Cell["\<\
In words this says something like,  \[OpenCurlyDoubleQuote]the rate of change \
of composed functions is the product of the rates of changes of the \
individual functions (at the appropriate points)\[CloseCurlyDoubleQuote]. The \
question one should ask is why is this result true? One method used to \
provide an explanation is to write something like the following on the board:\
\>", "Text",
 CellChangeTimes->{{3.525953191983*^9, 3.525953377441*^9}, {3.525953433406*^9,
